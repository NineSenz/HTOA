<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ht.bean">

    <class name="T_beonduty" table="t_beonduty">
        <id name="t_beonduty_id" column="t_beonduty_id"></id>
        <property name="t_BeondutyTime" column="t_BeondutyTime" />
        <property name="t_scope" column="t_scope" />
        <property name="t_record" column="t_record" />

        <many-to-one name="t_staff" class="com.ht.bean.T_staff" column="t_staff_id"></many-to-one>
    </class>

</hibernate-mapping>