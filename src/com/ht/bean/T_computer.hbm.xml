<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ht.bean">
    <class name="T_computer" table="t_computer" >
        <id name="t_comp_id" column="t_comp_id"></id>
        <property name="t_comp_stuid" column="t_comp_stuid" />
        <property name="t_comp_component" column="t_comp_component" />
        <property name="t_comp_date" column="t_comp_date" />
    </class>
</hibernate-mapping>