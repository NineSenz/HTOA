<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">


<hibernate-mapping package="com.ht.bean">
    <class name="T_tickling" table="t_tickling">
        <id name="t_tickling_id" column="t_tickling_id"/>
        <property name="t_matter" column="t_matter"/>
        <property name="t_result" column="t_result"/>

        <many-to-one name="t_staff" class="com.ht.bean.T_staff" column="t_staff_id"/>
    </class>
</hibernate-mapping>
