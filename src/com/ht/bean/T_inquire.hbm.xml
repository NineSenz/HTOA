<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ht.bean">
    <class name="T_inquire" table="t_inquire">
        <id name="t_inq_id" column="t_inq_id"></id>
        <property name="t_staff_id" column="t_staff_id"></property>
        <property name="t_inq_name" column="t_inq_name"></property>
        <property name="t_inq_time" column="t_inq_time"></property>
    </class>
</hibernate-mapping>