<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.ht.bean">
    <class name="T_journal" table="t_journal">
        <id name="t_log_id" column="t_log_id"></id>
        <property name="t_staff_id" column="t_staff_id"></property>
        <property name="t_date" column="t_date"></property>
        <property name="t_WeekSeven" column="t_WeekSeven"></property>
        <property name="t_WeekOne" column="t_WeekOne"></property>
        <property name="t_WeekTwo" column="t_WeekTwo"></property>
        <property name="t_WeekThree" column="t_WeekThree"></property>
        <property name="t_WeekFour" column="t_WeekFour"></property>
        <property name="t_WeekFive" column="t_WeekFive"></property>
        <property name="t_WeekSix" column="t_WeekSix"></property>
        <property name="t_summary" column="t_summary"></property>

        <!--<many-to-one name="t_staff" class="com.ht.bean.T_staff" column="t_sta_id"></many-to-one>-->
    </class>
</hibernate-mapping>